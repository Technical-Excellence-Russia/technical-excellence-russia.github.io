{
  "name": "teru",
  "version": "1.0.0",
  "description": "Source for https://technical-excellence.ru",
  "private": true,
  "scripts": {
    "lint": "npx eslint ./src ./test --fix",
    "dev": "npx webpack --mode development",
    "watch": "npx webpack --mode development --watch",
    "test": "npx mocha",
    "coverage": "npx nyc mocha && nyc report --reporter=text-lcov > coverage.lcov",
    "prod": "npm run lint && npm run test && npx webpack --mode production",
    "start": "webpack serve --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:timmson/teru.git"
  },
  "author": "timmnson",
  "license": "MIT",
  "bugs": {
    "url": "https://git@github.com:timmson/teru/issues"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.7",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "autoprefixer": "^9.7.6",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "chai": "^4.2.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "mini-css-extract-plugin": "^1.3.2",
    "mocha": "^8.2.1",
    "node-sass": "^5.0.0",
    "nyc": "^15.1.0",
    "postcss": "^8.1.14",
    "precss": "^4.0.0",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
